{
  "version": "1.6.1",
  "description": "package-test-utils-ts",
  "name": "package-test-utils-ts",
  "preinstall": "npx only-allow pnpm",
  "minimumReleaseAge": 10800,
  "scripts": {
    "clean": "pnpm -r clean  && rm -rf node_modules apps/*/node_modules shared/*/node_modules && pnpm run docker:clean",
    "docker:clean": "docker image rm -f web-js web-cs web-ts",
    "web-ts": "pnpm --filter web-ts",
    "web-cs": "pnpm --filter web-cs",
    "web-js": "pnpm --filter web-js",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "serve:all": "/usr/bin/terminator -u -l ptu 2> /dev/null > /dev/null &",
    "update": "npx npm-check-updates -i -t minor",
    "update:majors": "npx npm-check-updates -i -t latest"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@types/jest": "30.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "30.2.0",
    "babel-loader": "10.0.0",
    "babel-preset-env": "7.0.0-beta.3",
    "css-loader": "7.1.2",
    "html-webpack-plugin": "5.6.5",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jsdom": "27.2.0",
    "prettier": "3.7.3",
    "raw-loader": "4.0.2",
    "sass": "1.94.2",
    "sass-loader": "16.0.6",
    "style-loader": "4.0.0",
    "webpack": "5.103.0",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2"
  },
  "dependencies": {
    "@hansogj/abonnement-js": "4.7.3",
    "@hansogj/array.utils": "2.7.3",
    "@hansogj/find-js": "6.7.3",
    "@hansogj/maybe": "2.5.1"
  },
  "engines": {
    "node": "^18 || ^20 || ^21 || ^22 || ^25"
  },
  "volta": {
    "node": "25.0.0"
  },
  "packageManager": "pnpm@10.21.0"
}