{
  "version": "1.3.8",
  "description": "package-test-utils-ts",
  "name": "package-test-utils-ts",
  "scripts": {
    "serve:ts": "webpack serve --config typescript/webpack.config.js",
    "test:ts": "jest --config=./typescript/jest.config.ts",
    "test:ts:watch": "npm run test:ts -- --watchAll",
    "serve:cjs": "webpack serve --config commonjs/webpack.config.js",
    "test:cjs": "jest --config=./commonjs/jest.config.js",
    "test:cjs:watch": "npm run test:cjs -- --watchAll",
    "update": "npx npm-check-updates -i -t minor && npm run post:update",
    "update:majors": "npx npm-check-updates -i -t latest && npm run post:update",
    "post:update": "rm -rfv node_modules && npm i --progress"
  },
  "devDependencies": {
    "@babel/core": "7.22.20",
    "@babel/preset-env": "7.22.20",
    "@babel/preset-typescript": "7.22.15",
    "@types/jest": "29.5.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "29.7.0",
    "babel-loader": "8.3.0",
    "babel-preset-env": "7.0.0-beta.3",
    "css-loader": "6.8.1",
    "html-webpack-plugin": "5.5.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jsdom": "20.0.3",
    "prettier": "2.8.8",
    "raw-loader": "4.0.2",
    "sass": "1.67.0",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "dependencies": {
    "@hansogj/abonnement-js": "4.0.0",
    "@hansogj/array.utils": "2.0.0",
    "@hansogj/find-js": "6.0.0",
    "@hansogj/maybe": "2.2.1"
  }
}
