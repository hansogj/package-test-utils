{
  "version": "1.4.8",
  "description": "package-test-utils-ts",
  "name": "package-test-utils-ts",
  "preinstall": "npx only-allow pnpm",
  "scripts": {
    "clean": "pnpm -r clean  && rm -rf node_modules apps/*/node_modules shared/*/node_modules && pnpm run docker:clean",
    "docker:clean": "docker image rm -f web-js web-cs web-ts",
    "web-ts": "pnpm --filter web-ts",
    "web-cs": "pnpm --filter web-cs",
    "web-js": "pnpm --filter web-js",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "serve:all": "/usr/bin/terminator -u -l ptu 2> /dev/null > /dev/null &",
    "update": "npx npm-check-updates -i -t minor",
    "update:majors": "npx npm-check-updates -i -t latest",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "7.28.3",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-typescript": "7.27.1",
    "@types/jest": "29.5.14",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "29.7.0",
    "babel-loader": "9.2.1",
    "babel-preset-env": "7.0.0-beta.3",
    "css-loader": "7.1.2",
    "html-webpack-plugin": "5.6.4",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jsdom": "27.0.1",
    "prettier": "3.6.2",
    "raw-loader": "4.0.2",
    "sass": "1.90.0",
    "sass-loader": "14.2.1",
    "style-loader": "4.0.0",
    "webpack": "5.101.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.2.2"
  },
  "dependencies": {
    "@hansogj/abonnement-js": "4.7.2",
    "@hansogj/array.utils": "2.7.2",
    "@hansogj/find-js": "6.7.2",
    "@hansogj/maybe": "2.4.4",
    "http-server": "14.1.1"
  },
  "engines": {
    "node": "^18 || ^20 || ^ 21"
  },
  "volta": {
    "node": "21.7.3"
  },
  "packageManager": "pnpm@10.13.1"
}
